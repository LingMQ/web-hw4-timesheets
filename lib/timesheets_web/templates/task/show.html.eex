<h1>Show Task</h1>

<ul>

  <li>
    <strong>Hours:</strong>
    <%= @task.hours %>
  </li>

  <li>
    <strong>Jobcode:</strong>
    <%= @task.jobcode %>
  </li>

  <li>
    <strong>Worker_ID:</strong>
    <%= @task.worker %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.task_path(@conn, :edit, @task) %></span>
<span><%= link "Back", to: Routes.task_path(@conn, :index) %></span>
